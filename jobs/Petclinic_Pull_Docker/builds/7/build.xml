    <script>pipeline {
  agent { label &apos;target2&apos;}
  stages {
    
    stage(&apos;Verification&apos;) {
       agent { label &apos;target2&apos;}
       steps {
                sh &apos;whoami&apos;
                sh &apos;pwd&apos;
        }
    }
    
    stage(&apos;Docker Pull&apos;) {
      agent { label &apos;target2&apos;}
      steps {
        sh &apos;sudo docker pull jpsanchez2024/petclinic:latest&apos; 
      }
    }
    
    stage(&apos;Docker Run&apos;) {
      agent { label &apos;target2&apos;}
      steps {
        script {
                    // Lancer la commande Docker run
                    def dockerRun = sh(script: &apos;sudo docker run --rm -d -p 8082:8080 jpsanchez2024/petclinic&apos;, returnStatus: true)
                    
                    // Vérifier le code de retour de la commande Docker run
                    if (dockerRun == 0) {
                        echo &quot;La commande Docker run s&apos;est terminée avec succès. Le pipeline se termine maintenant.&quot;
                        return // Terminer le pipeline avec succès
                    } else {
                        currentBuild.result = &apos;FAILURE&apos; // Marquer le build comme échoué
                        error &quot;La commande Docker run a échoué avec le code de sortie ${dockerRun}&quot;
                    }
                    
                }
      }
    }
  }
}  
  </script>
